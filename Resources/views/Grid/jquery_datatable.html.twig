<table id="{{ id }}" {{ _self.render_attb(table_attr) }} data-jqtable="{{ options | json_encode }}">
	<thead>
		<tr>
			{% for column in columns %}
			<th data-column-field="{{ column.field }}">
				{{ column.getLabel() }}
			</th>
			{% endfor %}
		</tr>
	</thead>
</table>

{% macro render_attb(attributes) %}
{% spaceless %}
{% for attb, value in attributes %}
    {{ attb }}="{{ value }}"
{% endfor %}
{% endspaceless %}
{% endmacro %}